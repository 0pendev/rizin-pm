R2PM_BEGIN

R2PM_GIT "https://github.com/radareorg/r2dec-js"
R2PM_DESC "[r2-plugin] Experimental Decompiler written in javascript on top of r2"
BINDIR="${R2PM_BINDIR}"

R2PM_INSTALL() {
	rm -rf core2
	git clone --branch core2 https://github.com/radareorg/r2dec-js core2
	cd core2 || exit 1
	make clean
	make || exit 1
	make install || exit 1
}

R2PM_UNINSTALL() {
	make uninstall || exit 1
}

R2PM_END
