R2PM_BEGIN

R2PM_GIT "https://github.com/radareorg/r2dec-js"
R2PM_DESC "[r2-plugin] Experimental Decompiler written in javascript on top of r2"
BINDIR="${R2PM_BINDIR}"

R2PM_INSTALL() {
	git checkout core2
	make clean && make -C p clean
	make -C p || exit 1
}

R2PM_UNINSTALL() {
	make -C p uninstall || exit 1
}

R2PM_END
